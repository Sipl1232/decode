.css-18nc3u2 .MuiOutlinedInput-root .MuiAutocomplete-input {
    background-color: var(--field);
    border-radius: 3px !important;
    color: var(--light-text);
    font-size: 10px !important;
    padding: 3px 5px !important;
}

.css-p1olib-MuiAutocomplete-endAdornment {
    transform: translate(0,0) !important;
}

.searchMenu {
    display: block;
    width: 100%;
    padding: .375rem .75rem;
    margin-bottom: 5px;
    font-size: 1rem;
    font-weight: 600;
    line-height: 1.5;
    outline-color: black;
    color: var(--bs-body-color);
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    background-color: var(--bs-body-bg);
    background-clip: padding-box;
    border: var(--bs-border-width) solid var(--bs-border-color) !important;
    border-radius: var(--bs-border-radius);
}

.file {
    color: #606060;
    width: 100%;
    height: 160px;
    border: 1px solid #ccc;
    margin: 0 0 8px;
    padding: 0 8px;
    display: block;
    position: relative;
    font-size: 16px;
    text-align: center;
    box-sizing: border-box;
    background: #eee;
    transition: border .1s ease;
    font-weight: 700;
    line-height: 158px;
}

    .file .counter {
        right: 2px;
        bottom: 2px;
        color: #eee;
        padding: 2px 6px;
        position: absolute;
        font-size: 12px;
        background: #ccc;
        font-weight: 700;
        line-height: 14px;
    }
    .file .detail {
        display: inline-block;
        overflow: hidden;
        max-width: 100%;
        white-space: nowrap;
        text-overflow: ellipsis;
    }
.file input[type=file] {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    cursor: pointer;
    opacity: 0;
    display: block;
    position: absolute;
}
.state[data-state] {
    width: 100%;
    border: 1px solid #ccc;
    padding: 8px 12px;
    display: block;
    box-sizing: border-box;
    background: #eee;
    line-height: 21px;
}
.select-background {
    background-color: rgba(0, 0, 0, 0.125) !important;
}